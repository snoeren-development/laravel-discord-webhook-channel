{
    "name": "snoeren-development/laravel-discord-webhook-channel",
    "description": "Send notifications to a Discord webhook.",
    "keywords": [
        "laravel",
        "notification",
        "driver",
        "discord",
        "webhook",
        "snoeren"
    ],
    "homepage": "https://github.com/snoeren-development/laravel-discord-webhook-channel",
    "license": "MIT",
    "authors": [
        {
            "name": "Snoeren Development",
            "email": "info@snoerendevelopment.nl"
        }
    ],
    "require": {
        "php": "^8.2",
        "guzzlehttp/guzzle": "^7.9.2",
        "illuminate/support": "^10.0||^11.0||^12.0"
    },
    "require-dev": {
        "larastan/larastan": "^2.9",
        "orchestra/testbench": "v9.5.2",
        "phpunit/phpunit": "^11.4.3",
        "spatie/macroable": "^2.0.0"
    },
    "autoload": {
        "psr-4": {
            "SnoerenDevelopment\\DiscordWebhook\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SnoerenDevelopment\\DiscordWebhook\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit ./tests && vendor/bin/phpstan"
    },
    "config": {
        "sort-packages": true
    }
}
